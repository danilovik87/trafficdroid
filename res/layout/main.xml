<?xml version="1.0"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/linearLayout" android:orientation="vertical"
	android:layout_width="fill_parent" android:layout_height="fill_parent">
	<ScrollView android:layout_width="fill_parent"
		android:layout_weight="1" android:layout_height="wrap_content">
		<TableLayout android:id="@+id/zonelist"
			android:shrinkColumns="1" android:layout_width="fill_parent"
			android:background="#333333" android:layout_height="wrap_content"
			android:stretchColumns="*" />
	</ScrollView>
</LinearLayout>